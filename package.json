{
  "name": "SCMobile",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-react-native",
    "setupFiles": [
      "./test-setup.js"
    ],
    "preprocessorIgnorePatterns": [
      "node_modules/(?!react-native|react-clone-referenced-element|tcomb-form-native|spatialconnect-form-schema|react-native-slider|react-native-button)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ios.js"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub"
    }
  },
  "dependencies": {
    "color": "0.11.3",
    "jwt-decode": "2.0.1",
    "react": "15.3.1",
    "react-native": "0.34.0",
    "react-native-button": "^1.6.0",
    "react-native-drawer": "^2.2.3",
    "react-native-image-picker": "^0.22.9",
    "react-native-maps": "file:../react-native-maps",
    "react-native-router-flux": "3.36.0",
    "react-native-slider": "^0.8.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "rx": "^4.1.0",
    "slash": "^1.0.0",
    "spatialconnect": "file:../spatialconnect-js",
    "spatialconnect-form-schema": "2.2.6",
    "tcomb-form-native": "^0.5.1",
    "tcomb-json-schema": "^0.3.0",
    "turf-bbox": "3.0.12",
    "turf-distance": "3.0.12",
    "turf-linestring": "1.0.2",
    "turf-point": "2.0.1",
    "turf-polygon": "1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-jest": "15.0.0",
    "babel-preset-react-native": "1.9.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "jest": "15.1.1",
    "jest-react-native": "15.0.0",
    "jsdom": "^8.4.1",
    "react-test-renderer": "15.3.0",
    "redux-mock-store": "1.1.2",
    "should": "9.0.2"
  }
}
