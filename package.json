{
  "name": "SCMobile",
  "version": "1.11.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma es5 --print-width 100 --write \"{,!(node_modules|public|android|ios|tools)/**/}*.js\""
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --print-width 100 --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./test-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation|react-clone-referenced-element|tcomb-form-native|spatialconnect-form-schema|react-native-slider|react-native-button)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ios.js"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub"
    }
  },
  "dependencies": {
    "color": "1.0.3",
    "husky": "0.13.4",
    "jwt-decode": "2.0.1",
    "lint-staged": "3.6.1",
    "lodash": "4.17.2",
    "prettier": "1.4.4",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-button": "1.7.1",
    "react-native-drawer": "^2.2.3",
    "react-native-image-picker": "^0.26.3",
    "react-native-maps": "boundlessgeo/react-native-maps",
    "react-native-slider": "^0.8.0",
    "react-native-spatialconnect": "0.11.2",
    "react-native-vector-icons": "4.0.0",
    "react-navigation": "1.0.0-beta.9",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "rx": "^4.1.0",
    "should": "9.0.2",
    "slash": "^1.0.0",
    "spatialconnect-form-schema": "3.0.0-beta.3",
    "tcomb-form-native": "0.6.9",
    "tcomb-json-schema": "^0.3.0",
    "turf-bbox": "3.0.12",
    "turf-distance": "3.0.12",
    "turf-linestring": "1.0.2",
    "turf-point": "2.0.1",
    "turf-polygon": "1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "jest": "18.1.0",
    "jsdom": "^8.4.1",
    "react-test-renderer": "15.4.2",
    "redux-mock-store": "1.1.2"
  }
}
